<div id="SENbody" img src="US_Capitol.jpg" alt="US Capitol <-- with an 'o' of course" /> 


<h1>All Scumbags</h1>

<% @scumbags.each do |scumbag| %>
  <div class="">
    <h5><%= scumbag.name %> <%= scumbag.party_abbreviation %></h5>
    <p>Price: $<%= scumbag.price %></p>
    <%= button_to "Add to Cart", cart_index_path(scumbag_id: scumbag.id), class: "btn btn-primary" %>
  </div>
<% end %>
